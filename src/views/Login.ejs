<!DOCTYPE html>
<html lang="es">

<head>
  <%- include('./head') %>
<link rel="stylesheet" href="css/auth.css">
</head>

<body>
  <header class="header shop">
    <%- include('./Barsup') %>
  </header>  
    <main class="py-5 login">
      <div class="card shadow my-5" style="min-width: 300px;">
        <form class="card-body" action="/login" method="POST">
          <h2 class="mt-3 h2 text-center">Ingresar</h2>
          <div class="md-form">
            <input type="text" id="Usuario" name="Usuario" class="form-control">
            <label for="firstName" class="">Email</label>
          </div>
          <div class="md-form">
            <input type="password" name="contrasena" id="contrasena" class="form-control">
            <label for="password">Password</label>
          </div>
          <hr class="mb-4">
          <button class="btn btn-primary btn-lg btn-block waves-effect waves-light" type="submit">Ingresar</button>
        </form>
      </div>
    </main>
      <%- include('./scripts') %>
</body>

</html>